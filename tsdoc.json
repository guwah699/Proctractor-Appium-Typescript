{
	"tags":
	{
		"allowUnknownTags" : true
	},
	"plugins":[
		 "plugins/markdown"
		,"./node_modules/tsdoc/template/plugins/TSDoc.js"
	],
	"opts":
	{
		"template"	:"./node_modules/tsdoc/template",
		"recurse"	:"true"
	},
	"templates" : {
		"cleverLinks"     : false,
		"monospaceLinks"  : false
	},
	"source":
	{
		"includePattern": "(\\.d)?\\.ts$",
		"excludePattern": "node_modules|step_definitions|PageObjects"
	},
	"markdown"  : {
		"parser"   : "gfm",
		"hardwrap" : true
	},
	"tsdoc":{
		"source"		:"./",
		"destination"	:"./out",
		"tutorials"		:"",
		"systemName"	: "automated-testing",
		"footer"		: "",
		"copyright"		: "automated-testing Copyright Â© 2016",
		"outputSourceFiles" : true,
		"commentsOnly": true
	}
}
